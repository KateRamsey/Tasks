@model IEnumerable<Tasks.Models.Task>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@foreach (var t in Model)
{
    if (t.IsFinished)
    {
        <li style="text-decoration: line-through">@t.Description (Owner @t.Name)</li>
    }
    else
    {
        <li>@t.Description (Owner @t.Name)</li>
    }
}

